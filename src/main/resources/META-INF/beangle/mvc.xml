<?xml version="1.0"?>
<mvc>
  <profile name="openurp-qos-evaluation" package="org.openurp.qos.evaluation.*.web.action">
    <url path="/" suffix=""  style="seo" />
    <interceptors>
      <interceptor name="web.Interceptor.hibernate"/>
      <interceptor name="web.Interceptor.security"/>
     </interceptors>
  </profile>
  <static>
    <bundle name="urp" version="default"/>
    <bundle name="echarts" version="4.5.0">
      <module name="echarts" js="dist/echarts.js"/>
    </bundle>
  </static>
</mvc>
