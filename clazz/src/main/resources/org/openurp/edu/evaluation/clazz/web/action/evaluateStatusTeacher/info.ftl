[#ftl]
[@b.head/]
[@b.toolbar title='学生评教详细信息' id='adClassStateInfoBar']
    bar.addBack();
[/@]
[@b.grid items=questionnaireTaskList var="courseTake" sortable="true"]
        [@b.row]
            [@b.col property="clazz.crn" title="课程序号"/]
            [@b.col property="clazz.course.name" title="课程名称"/]
            [@b.col property="" title="教学班"]
            ${(courseTake.clazz.teachclass.name)!}
            [/@]
            [@b.col property="" title="教师"]
            [#list courseTake.clazz.teachers as teacher]
            ${teacher.user.name}<br>
            [/#list]
            [/@]
            [@b.col property="clazz.teachclass.stdCount" title="班级人数"/]
            [@b.col property="clazz.course.credits" title="学分"/]
            [@b.col property="" title="是否已测评"]
            [#assign num =0]
      [#list evaluateResultList as evaluateR]
      [#if evaluateR.clazz.id == courseTake.clazz.id]
      [#assign num = num +1]
      [/#if]
      [/#list]
      [#if 0 < num]
          已评测
      [#else]
      <font color="red">未评测</font>
      [/#if]
            [/@]
     [/@]
[/@]
[@b.foot/]
